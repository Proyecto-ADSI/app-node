<div class="row">
    <div class="col-lg-12">
        <div class="card card-outline-success">
            <div class="card-header MyStyle_Card_Header">
                <h4 class="m-b-0 text-white">Formulario de registro de usuarios</h4>
            </div>
            <div class="card-body">
                <form id="FormRegistroUsuario" enctype="multipart/form-data">
                    <div class="form-body ml-5 mr-5">
                        <h3 class="box-title">Empleado:</h3>
                        <hr class="m-t-0 m-b-40">

                        <ul class="nav nav-tabs customtab" role="tablist">
                            <li class="nav-item"> <a id="FormularioEmpleado" class="nav-link active" data-toggle="tab"
                                    href="#formEmpleado" role="tab">
                                    <span class="hidden-sm-up"><i class="ti-home"></i></span>
                                    <span class="hidden-xs-down">Registrar empleado</span></a>
                            </li>
                            <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#selectEmpleado"
                                    role="tab">
                                    <span class="hidden-sm-up"><i class="ti-user"></i></span>
                                    <span class="hidden-xs-down">Seleccionar empleado </span></a>
                            </li>
                        </ul>
                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div class="tab-pane active m-3" id="formEmpleado" role="tabpanel">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label text-right" id="Label-Form-Registro-Usuario">
                                                Tipo de documento<span class="text-danger">*</span>
                                            </label>

                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="ti-user"></i>
                                                    </span>
                                                </div>
                                                <select class="form-control custom-select" id="txtTipoDocumento"
                                                    name="txtTipoDocumento">

                                                </select>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Documento
                                                <span class="text-danger">*</span>
                                            </label>

                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="ti-credit-card"></i>
                                                    </span>
                                                </div>
                                                <input type="number" class="form-control" id="txtDocumento"
                                                    name="txtDocumento" placeholder="Número de documento">
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!--/row-->
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Nombre
                                                <span class="text-danger">*</span>
                                            </label>
                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="ti-user"></i>
                                                    </span>
                                                </div>
                                                <input type="text" class="form-control" id="txtNombre" name="txtNombre"
                                                    placeholder="Nombre Completo">
                                            </div>
                                        </div>
                                    </div>
                                    <!--/row-->
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Apellidos
                                                <span class="text-danger">*</span>
                                            </label>

                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="ti-user"></i>
                                                    </span>
                                                </div>
                                                <input type="text" class="form-control" id="txtApellidos"
                                                    name="txtApellidos" placeholder="Apellidos">
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <!--/row-->
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Correo
                                                <span class="text-danger">*</span>
                                            </label>

                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="ti-email"></i>
                                                    </span>
                                                </div>
                                                <input type="email" class="form-control" id="txtEmail" name="txtEmail"
                                                    placeholder="Ingresar correo">
                                            </div>

                                        </div>

                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Sexo
                                                <span class="text-danger">*</span>
                                            </label>

                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="icon-star"></i>
                                                    </span>
                                                </div>
                                                <select class="form-control custom-select" id="txtSexo" name="txtSexo">

                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- /Row -->
                                <div class="row">
                                    <div class="col-md-6">

                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Celular
                                            </label>
                                            <div class="input-group ">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class="ti-mobile"></i>
                                                    </span>
                                                </div>
                                                <input type="number" class="form-control" id="txtCelular"
                                                    name="txtCelular" placeholder="Ingresar número">
                                            </div>

                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" id="Label-Form-Registro-Usuario">Turno
                                                <span class="text-danger">*</span>
                                            </label>

                                            <div class="input-group">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <i class=" icon-clock "></i>
                                                    </span>
                                                </div>
                                                <select class="form-control custom-select" id="txtTurno"
                                                    name="txtTurno">

                                                </select>
                                            </div>


                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-6">
                                        <div class="card" id="Card-Fotografia">
                                            <div class="card-body MyStyle_File_Card">
                                                <h4 class="card-title">Fotografía</h4>
                                                <input type="file" id="fileFotografia" name="Fotografia" class="dropify"
                                                    data-height="100" />
                                                <!-- data-default-file="../../assets/plugins/dropify/src/images/test-image-2.jpg" -->
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <!-- /row -->
                            </div>
                            <div class="tab-pane  p-20" id="selectEmpleado" role="tabpanel">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="ListaEmpleados" class="control-label">
                                                Empleados registrados
                                                <span class="text-danger">*</span>
                                            </label>
                                            <select class="ListarEmpleados form-control custom-select"
                                                name="ListaEmpleados" id="ListaEmpleados"
                                                style="width: 100%; height:36px;">
                                            </select>

                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>


                        <h3 class="box-title">Usuario:</h3>
                        <hr class="m-t-0 m-b-40">
                        <!--/row-->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label" id="Label-Form-Registro-Usuario">Usuario
                                        <span class="text-danger">*</span>
                                    </label>
                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="basic-addon1">
                                                <i class="ti-user"></i>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control" id="txtUsuario" name="txtUsuario"
                                            placeholder="Nombre usuario">
                                    </div>

                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label" id="Label-Form-Registro-Usuario">Tipo de usuario
                                        <span class="text-danger">*</span>
                                    </label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="basic-addon1">
                                                <i class="icon-people "></i>
                                            </span>
                                        </div>
                                        <select class="form-control custom-select" id="txtRol" name="txtRol">

                                        </select>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <!-- /Row -->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label" id="Label-Form-Registro-Usuario">Contraseña
                                        <span class="text-danger">*</span>
                                    </label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="basic-addon1">
                                                <i class="ti-lock"></i>
                                            </span>
                                        </div>
                                        <input type="password" class="form-control" id="txtContrasena" autocomplete=""
                                            name="txtContrasena" placeholder="Contraseña">
                                    </div>
                                </div>
                            </div>
                            <!--/span-->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label" id="Label-Form-Registro-Usuario">Confirmar
                                        <span class="text-danger">*</span>
                                    </label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="basic-addon1">
                                                <i class="ti-lock"></i>
                                            </span>
                                        </div>
                                        <input type="password" class="form-control" id="txtConfirmarContrasena"
                                            name="txtConfirmarContrasena" autocomplete=""
                                            placeholder="Confirmar contraseña">
                                    </div>

                                </div>
                            </div>
                            <!--/span-->
                        </div>
                        <!--/row-->
                        <hr>
                        <div class="form-actions row">
                            <div class="col-md-6 MyStyle_Btn_Registro_Usuarios">
                                <div class="row" style="margin-top: 0.5rem; position: relative; top: -2rem;">
                                    <div class="col-md-offset-3 col-md-9">
                                        <button type="submit" class="btn btn-info">Guardar</button>
                                        <a href="GestionarUsuarios.html" style="margin-left:1rem;"
                                            class="btn btn-danger">Cancelar</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /Form Body -->
                </form>
            </div>
        </div>
    </div>
</div>