<div class="row">
    <div class="col-md-10">
        <div class="card">
            <div class="card-body wizard-content">
                <div class="row">
                    <div class="col-md-3">
                        <h2 class="card-title titulo_RL">Registro de llamadas</h2>
                    </div>
                    <div class="col-md-4">
                        <h5 class="titulo_RL">
                            <i class="fa fa-clock-o"></i> Llamada
                        </h5>
                        <div class="MyStyle_Contador">
                            <div id="txtMinutosL" class="Contador_Minutos">00</div>
                            <div id="txtSegundosL" class="Contador_Segundos">00</div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <h6 class="titulo_RL">
                            <i class="fa fa-search"></i> Post-llamada
                        </h6>
                        <div class="MyStyle_Contador">
                            <div id="txtMinutosV" class="Contador_Minutos">00</div>
                            <div id="txtSegundosV" class="Contador_Segundos">00</div>
                        </div>
                    </div>
                    <div class="col-md-2 btns">
                        <button type="button" id="btnTerminarLlamada" data-toggle="tooltip"
                            data-original-title="Terminar Llamada" class="btn btn-danger d-inline-block">
                            <i class="fa fa-close"></i>
                        </button>
                    </div>
                </div>
                <hr class="m-t-10 m-b-20">
                <form id="Form_Registro_LlamadaNP" class="wizard-circle">
                    <!-- Step 1 -->
                    <h6>Información de contacto</h6>
                    <section>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">Razón social:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="text" name="txtRazonSocial" id="txtRazonSocial" class="form-control"
                                        placeholder="Empresa S.A" autocomplete="on">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="emailAddress1"> Teléfono:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="number" class="form-control" id="txtTelefono" name="txtTelefono"
                                        placeholder="4123456">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="int1">Extensión:</label>
                                    <input type="text" class="form-control" id="txtExtension" name="txtExtension"
                                        placeholder="Ext: 101">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">
                                        NIT:
                                    </label>
                                    <input type="text" id="txtNIT" name="txtNIT" class="form-control"
                                        placeholder="12345678-9">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="txtPersona_Responde">¿Quién responde?:</label>
                                    {{!-- <span class="text-danger">*</span> --}}
                                    <input type="checkbox" id="txtValEncargado" name="txtValEncargado"
                                        class="MyStyle_Checkbox">
                                    <label for="txtValEncargado" id="lbltxtValEncargado" class="checkbox"
                                        data-toggle="tooltip" data-original-title="¿Encargado?"></label>
                                    <input type="text" class="form-control" id="txtPersona_Responde"
                                        name="txtPersona_Responde" placeholder="Jhon Doe">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="int1">Encargado:</label>
                                    <input type="text" class="form-control" id="txtEncargado" name="txtEncargado"
                                        placeholder="Jhon Doe">
                                </div>
                            </div>
                        </div>
                        <h4 class="box-title d-inline-block">Ubicación:</h4>
                        <input type="radio" id="Validacion_UbicacionEmpresa" name="Validacion_UbicacionEmpresa"
                            class="with-gap">
                        <label for="Validacion_UbicacionEmpresa"
                            id="lblValidacion_UbicacionEmpresa">Colombia/Antioquia/Medellín/Barrio</label>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">País:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtPais" id="txtPais" class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Departamento:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtDepartamento" id="txtDepartamento"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Municipio:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtMunicipio" id="txtMunicipio"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Barrio o vereda:
                                    </label>
                                    <select name="txtSubTipo" id="txtSubTipo"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                        </div>
                        <!-- /Row -->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Nombre barrio o vereda</label>
                                    <select name="txtNombre_Lugar" id="txtNombre_Lugar"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="emailAddress1"> Dirección:</label>
                                    <input type="text" class="form-control" id="txtDireccion" name="txtDireccion"
                                        readonly data-toggle="modal" data-target="#modalDireccion"
                                        placeholder="Dirección">
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Step 2 -->
                    <h6>Datos básicos líneas</h6>
                    <section>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-3 z_indexCorrecionTooltip">
                                <div class="form-group">
                                    <label for="location1">¿Corporativo?</label>
                                    <br>
                                    <input type="checkbox" class="switch_corporativo" disabled>
                                </div>
                            </div>
                            <div class="col-md-5">
                                <div class="form-group">
                                    <label class="control-label">Operador del
                                        <strong>cliente:</strong><span class="text-danger">*</span></label>
                                    <select name="txtOperador" id="txtOperador"
                                        class="select2 form-control custom-select valDetalle">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label"> Calificación del operador:
                                        {{!-- <span class="text-danger">*</span> --}}
                                    </label>
                                    <select name="txtCalificacion" id="txtCalificacion"
                                        class="select2 form-control custom-select valDetalle">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="control-label">
                                        Razones: <i class="fa fa-question-circle" data-toggle="tooltip"
                                            data-original-title="Utilice punto y coma o enter para guardar la
                                            entrada."></i>
                                    </label>
                                    <select id="txtRazones" class="select2-multiple m-b-10 valDetalle"
                                        style="width: 100%" multiple="multiple" data-placeholder="Seleccione o escriba">

                                    </select>
                                </div>
                            </div>
                        </div>
                        {{>registrar-detalle-servicios}}

                        <div class="row ValidacionClickCorp">
                            <div class="col-md-12 text-center">
                                <div class="form-group d-inline-block m-r-40 ValidacionAT">
                                    <label for="location1">¿Atención telefónica?</label>
                                    <br>
                                    <input type="checkbox" class="switch_AT1" disabled>
                                </div>
                                <div class="form-group d-inline-block">
                                    <label for="location1">¿Agenda cita?</label>
                                    <br>
                                    <input type="checkbox" class="switch_cita1" disabled>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Step 3 -->
                    <h6>Plan corporativo</h6>
                    <section>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="form-group ">
                                    <label class="control-label">¿Claúsula permanencia?:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <br>
                                    <div id="switchClausula" class="switch">
                                        <label>
                                            <strong> NO</strong>
                                            <input type="checkbox">
                                            <span class="lever"></span>
                                            <strong>SI</strong>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-7">
                                <div class="form-group">
                                    <label for="txtFecha_inicio">
                                        Fecha del corporativo:
                                    </label>
                                    <div class="input-daterange input-group" id="Fecha_Corporativo">
                                        <span class="input-group-addon">Inicio</span>
                                        <input type="text" class="input-sm form-control" name="txtFecha_inicio"
                                            id="txtFecha_inicio" placeholder="yyyy/mm/dd" />
                                        <span class="input-group-addon">Fin</span>
                                        <input type="text" class="input-sm form-control" name="txtFecha_fin"
                                            id="txtFecha_fin" placeholder="yyyy/mm/dd" />
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="participants1">Descripción del corporativo:</label>
                                    <textarea name="txtDescripcion" id="txtDescripcion" rows="5" class="form-control"
                                        placeholder="Ingresa una descripción"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 text-center">
                                <div class="form-group d-inline-block m-r-40 ValidacionAT">
                                    <label for="location1">¿Atención telefónica?</label>
                                    <br>
                                    <input type="checkbox" class="switch_AT2">
                                </div>
                                <div class="form-group d-inline-block">
                                    <label for="location1">¿Agenda cita?</label>
                                    <br>
                                    <input type="checkbox" class="switch_cita2">
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Step 4 -->
                    <h6>Cita</h6>
                    <section>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">
                                        Operador de la <strong>cita: </strong><span class="text-danger">*</span>
                                    </label>
                                    <select name="txtOperadorCita" id="txtOperadorCita"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-5">
                                <div class="form-group">
                                    <label class="control-label">Fecha:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="text" id="date-format" name="txtFechaCita"
                                        class="form-control floating-label" placeholder="Lunes 01 Enero 2020">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">Hora:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="button" name="btnHoraCita" id="btnHoraCita" data-toggle="modal"
                                        data-target="#modalHorasCitas" class="btn btn-info text-white form-control"
                                        value="Seleccione...">
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-5">
                                <div class="form-group">
                                    <label for="txtEncargado_Cita">¿Quién atiende la cita?:
                                        <span class="text-danger">*</span>
                                    </label>

                                    <input type="checkbox" id="txtValEncargadoCita" name="txtValEncargadoCita"
                                        class="MyStyle_Checkbox">
                                    <label for="txtValEncargadoCita" id="lbltxtValEncargadoCita" class="checkbox"
                                        data-toggle="tooltip" data-original-title="¿Encargado?"></label>

                                    <input type="text" class="form-control" id="txtEncargado_Cita"
                                        name="txtEncargado_Cita" placeholder="Nombre Completo">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label for="int1">Celular:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="text" class="form-control" id="txtCelularCita" name="txtCelularCita"
                                        placeholder="3123456788">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="location1">¿Es el representante legal?:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <div id="switchRL" class="switch">
                                        <label>
                                            <strong> NO</strong>
                                            <input type="checkbox">
                                            <span class="lever"></span>
                                            <strong>SI</strong>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <h4 class="box-title d-inline-block">Ubicación</h4>
                        <input name="Validacion_UbicacionCita" type="radio" class="with-gap" id="ValAtiendeEmpresa">
                        <label for="ValAtiendeEmpresa">Atiende en la empresa</label>
                        <input name="Validacion_UbicacionCita" type="radio" class="with-gap" id="Validacion_Ubicacion2">
                        <label for="Validacion_Ubicacion2">Colombia/Antioquia/Medellín/Barrio</label>

                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">País:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtPaisCita" id="txtPaisCita"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Departamento:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtDepartamentoCita" id="txtDepartamentoCita"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Municipio:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtMunicipioCita" id="txtMunicipioCita"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Barrio o vereda:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtSubTipoCita" id="txtSubTipoCita"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                        </div>
                        <!-- /Row -->
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label">Nombre barrio o vereda:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <select name="txtNombre_LugarCita" id="txtNombre_LugarCita"
                                        class="select2 form-control custom-select">
                                        <option selected disabled>Seleccione...</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="emailAddress1"> Dirección:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="text" class="form-control" id="txtDireccion_Cita" readonly
                                        name="txtDireccion_Cita" placeholder="Dirección">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="Location1">Punto de referencia:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <input type="text" class="form-control" id="txtPuntoReferencia"
                                        name="txtPuntoReferencia"
                                        placeholder="Centro Comercial, Iglesia, Parque, etc...">
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="font-weight-bold">
                                        <i class="fa fa-clock-o"></i> ¿Recordar cita?:</label>
                                    <input type="text" id="Fecha_RC" class="form-control floating-label"
                                        placeholder=" Ejemplo: Lunes 01 Enero 2020 12:00">
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Step 5 -->
                    <h6>Atención telefónica</h6>
                    <section>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label class="control-label">Envio de oferta:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <br>
                                    <input name="rbtnEnvioOferta" type="radio" class="with-gap" id="rbtnAtencionTel1"
                                        value="1">
                                    <label id="lblAtencionTel1" for="rbtnAtencionTel1">
                                        <i class="mdi mdi-email"></i> Correo
                                    </label>
                                    <br>
                                    <input name="rbtnEnvioOferta" type="radio" class="with-gap" id="rbtnAtencionTel2"
                                        value="2">
                                    <label id="lblAtencionTel2" for="rbtnAtencionTel2">
                                        <i class="mdi mdi-whatsapp"></i> WhatsApp
                                    </label>
                                    <br>
                                    <input name="rbtnEnvioOferta" type="radio" class="with-gap" id="rbtnAtencionTel3"
                                        value="3">
                                    <label id="lblAtencionTel3" for="rbtnAtencionTel3">
                                        <i class="mdi mdi-email"></i><i class="mdi mdi-whatsapp"></i> Ambos medios
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-8">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label class="control-label">Correo:
                                                <span class="text-danger">*</span>
                                            </label>
                                            <input type="text" name="txtCorreo" id="txtCorreo" class="form-control"
                                                disabled>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="int1">Código postal:
                                                <span class="text-danger">*</span>
                                            </label>
                                            <input type="text" class="form-control" id="txtCodigoPostal"
                                                name="txtCodigoPostal" placeholder="+57" value="57" disabled>
                                        </div>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label for="int1">Celular:
                                                <span class="text-danger">*</span>
                                            </label>
                                            <input type="text" class="form-control" id="txtCelularAT"
                                                name="txtCelularAT" disabled>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </section>
                    <!-- Step 6 -->
                    <h6>Finalizar llamada</h6>
                    <section>
                        <hr class="m-t-0 m-b-20">
                        <div class="row">
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label for="location1">Habeas Data:<span class="text-danger">*</span>
                                    </label>
                                    <input type="checkbox" class="switch_habeas_data">
                                </div>
                            </div>
                            <div class="col-md-3 ValidacionesCita" style="display: none;">
                                <div class="form-group">
                                    <label for="location1">¿Factura?:
                                        <span class="text-danger">*</span>
                                    </label>
                                    <br>
                                    <input type="checkbox" class="switch_factura">
                                </div>
                            </div>
                            <div class="col-md-3 ValidacionesLlamada">
                                <div class="form-group">
                                    <label for="location1">
                                        ¿Llamar nuevamente?</label>
                                    </label>
                                    <br>
                                    <input type="checkbox" class="switch_llamarNuevamente">
                                </div>
                            </div>
                            <div id="ValidacionTerminarLL1" class="col-md-7">
                                <div class="form-group">
                                    <label class="control-label">Conclusión Llamada:</label>
                                    <select name="txtConclusion" id="txtConclusion"
                                        class="select2 form-control custom-select" disabled="disabled">
                                        <option selected value="1">No efectiva</option>
                                        <option disabled value="2">Llamar nuevamente</option>
                                        <option disabled value="3">Efectiva</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3 ValidacionTerminarLL2" style="display: none;">
                                <div class="form-group">
                                    <label class="control-label">Conclusión Llamada:</label>
                                    <select name="txtConclusion" id="txtConclusion"
                                        class="select2 form-control custom-select" disabled="disabled">
                                        <option selected value="1">No efectiva</option>
                                        <option disabled value="2">Llamar nuevamente</option>
                                        <option disabled value="3">Efectiva</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4 ValidacionTerminarLL2" style="display: none;">
                                <div class="form-group">
                                    <label class="control-label">
                                        ¿Se llamó a una empresa? <span class="text-danger">*</span>
                                    </label>
                                    <br>
                                    <input name="rbtEmpresaValida" type="radio" class="with-gap" id="rbtValEmpresa"
                                        value="1">
                                    <label for="rbtValEmpresa">Si</label>
                                    <input name="rbtEmpresaValida" type="radio" class="with-gap" id="rbtValEmpresa2"
                                        value="2">
                                    <label for="rbtValEmpresa2">No</label>
                                    <input name="rbtEmpresaValida" type="radio" class="with-gap" id="rbtValEmpresa3"
                                        value="3">
                                    <label for="rbtValEmpresa3">No responden</label>
                                </div>
