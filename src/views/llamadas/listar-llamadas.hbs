            </div>
            <div class="card-body" id="Card-Body-Filtrar-Llamadas2">
                <div class="row">
                    <div class="col-md-4">
                        <div class="example">
                            <label class="m-t-30">NIT</label>
                            <input type="text" class="form-control" id="Filtro_NIT" placeholder="NIT">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="example">
                            <label class="m-t-30">Razón social</label>
                            <input class="form-control" type="text" id="Filtro_RazonSocial" placeholder="Razón social">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="example">
                            <label class="m-t-30">Municipio</label>
                            <input class="form-control" type="text" id="Filtro_Municipio" placeholder="Municipio">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <div class="example">
                            <label class="m-t-30">Fecha</label>
                            <input class="form-control" type="text" id="Filtro_Fecha" placeholder="Fecha">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="example">
                            <label class="m-t-30">Usuario</label>
                            <input class="form-control" type="text" id="Filtro_Usuario" placeholder="Usuario">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="example">
                            <label class="m-t-30">Estado</label>
                            <select name="txtCalificacion" id="Filtro_Estado"
                                class="select2 form-control custom-select valDetalle">
                                <option selected disabled>Seleccione...</option>
                                <option value="1">No efectiva</option>
                                <option value="2">Llamar nuevamente</option>
                                <option value="3">Efectiva</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group m-t-30 MyStyle_Btn_Filtro ">
                            <button type="button" onclick="LimpiarFiltro();" class="btn btn-info">
                                Limpiar filtro avanzado</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="table-responsive m-t-40">
            <table id="LlamadasDataTable" class="table stylish-table MyStyle_Color-Table">
                <thead>
                    <tr>
                        <th><i class="fa fa-address-card"></i> NIT</th>
                        <th><i class="fa fa-building-o"></i> Empresa</th>
                        <th><i class="ti-location-pin"></i> Municipio</th>
