<div class="card">
    <div class="card-header MyStyle_Card_Header">
        <h4 class="m-b-0 text-white">Registro de Llamadas</h4>
    </div>
    <div class="card-body" id="Card-Body-Filtrar-Llamadas">
        <div class="card MyStyle_Card_Filtro card-outline-primary">
            <div class="card-header">
                <h4 class="m-b-0 text-white">Filtro:</h4>
            </div>
            <div class="card-body" id="Card-Body-Filtrar-Llamadas2">
                <form class="" action="" method="POST">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="row">
                                <div class="col-md-6 form-group">
                                    <label class="m-t-30">NIT</label>
                                    <input type="text" class="form-control" id="Llamadas-Nit" placeholder="123...">
                                </div>
                                <div class="col-md-2">
                                    <label class="MyStyle_Guion"> -</label>
                                </div>
                                <div class="col-md-4">
                                    <label class="m-t-30">Dígito</label>
                                    <input type="number" class="form-control" id="Llamadas-Digito" placeholder="1...">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="example">
                                <label class="m-t-30">Rango de Fechas</label>
                                <input class="form-control input-daterange-datepicker" type="text" name="daterange"
                                    id="Llamadas-Rango-Fechas" value="" />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <label class="m-t-30">Hora</label>
                            <div class="input-group clockpicker " data-placement="bottom" data-align="top"
                                data-autoclose="true">
                                <input type="text" class="form-control" id="Llamadas-Hora" value="">
                                <div class="input-group-append">
                                    <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <label class="m-t-30">Teléfono</label>
                            <input type="text" class="form-control" id="Llamadas-Telefono" placeholder="1234567...">
                        </div>
                        <div class="col-md-4">
                            <label class="m-t-30">Contact Center</label>

                            <select class="selectpicker" id="Llamadas-Contact" multiple data-style="form-control"
                                style="width: 100%;">
                                <!--  select2 custom-select form-control -->
                                <!-- multiple data-style="form-control btn-secondary" -->
                                <!-- <option>Select</option> -->
                                <optgroup label="Hombres">
                                    <option value="CA">Juan Pérez</option>
                                    <option value="NV">Juan Manuel Rico</option>
                                    <option value="OR">Andrés Felipe Montoya</option>
                                    <option value="WA">Santiago Mejía</option>
                                </optgroup>
                                <optgroup label="Mujeres">
                                    <option value="AZ">Mónica Garcia</option>
                                    <option value="CO">Alejandra Parra</option>
                                    <option value="ID">Jhohana Vélez</option>

                                </optgroup>
                            </select>
                        </div>
                        <div class="col-md-4">
                            <h5 class="m-t-30">Estado</h5>
                            <select class="selectpicker form-control" id="Llamadas-Estado" style="width: 100%;">
                                <option disabled selected>Seleccione...</option>
                                <option>Efectiva</option>
                                <option>Programada</option>
                                <option>No Efectiva</option>
                            </select>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group m-t-30 MyStyle_Btn_Filtro ">
                                <button type="submit" class="btn btn-info"><i class="fas fa-search"></i>
                                    Consultar</button>
                                <button class="btn btn-warning" style="margin-left: 1.7rem;"><i
                                        class="fas fa-filter"></i> Filtro avanzado </button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="table-responsive m-t-40">
            <table id="LlamadasDataTable" class="table stylish-table MyStyle_Color-Table">
                <thead>
                    <tr>
                        <th><i class="fa fa-address-card"></i> NIT</th>
                        <th><i class="fa fa-building-o"></i> Empresa</th>
                        <th><i class="ti-location-pin"></i> Municipio</th>
                        <th><i class="fa fa-clock-o"></i> Fecha </th>
                        <th><i class="fa fa-user"></i> Usuario</th>
                        <th><i class="fa fa-list-ul"></i> Estado</th>
                        <th><i class="fa fa-sliders"></i> Opciones</th>
                    </tr>
                </thead>
